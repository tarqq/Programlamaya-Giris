public class Dongu13Katlari {

public static void main(String[] args) {
    /*100'den 0'a kadar 13'e tam bölünebilen sayıları ekrana yazdırınız 
     (büyükten küçüğe) */
  
  //Döngüyü karmaşık tutarak çözümü
  
  for(int i= 91;i>=1;i-=13){
    System.out.println("Çözüm1: "+ i);
    }
  
  //Sayılabilirlik Teorisi'ne (Countability Theory) uygun çözümü
  
  for (int i=0;i<7;i++) {
            System.out.println("Çözüm2: "+ 91-(13*i));
     }
    
    }
    
   }
